#include<stack>
class Solution
{
public:
    void push(int node) {
        s1.push(node);
    }

    int pop() {
        if(s2.size() <= 0)
        {
            while(s1.size() != 0)
            {
                int a = s1.top();
			s2.push(a);
			s1.pop();
            }
        }
        int c = s2.top();
	s2.pop();
	return c;
    }

private:
    stack<int> s1;
    stack<int> s2;
};
